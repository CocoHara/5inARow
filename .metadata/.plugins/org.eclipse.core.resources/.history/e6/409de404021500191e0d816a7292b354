package server;

public class Game 
{
	private int row;
	private int column;
	private char counter;

	public int getRow() 
	{
		return row;
	}

	public void setRow(int row) 
	{
		this.row = row;
	}

	public int getColumn() 
	{
		return column;
	}

	public void setColumn(int column) 
	{
		this.column = column;
	}

	public char getCounter() 
	{
		return counter;
	}

	public void setCounter(char counter) 
	{
		this.counter = counter;
	}	
}
