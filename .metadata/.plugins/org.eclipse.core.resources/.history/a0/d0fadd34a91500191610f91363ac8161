package server;

import javax.xml.bind.annotation.XmlRootElement;
import javax.xml.bind.annotation.XmlType;

@XmlRootElement(name = "rowcol")
//Optional
@XmlType(propOrder = { "row", "column", "counter" })

public class Game 
{
	private int row;
	private int column;
	private String counter;
	/*private boolean player1Turn;
